{
  "name": "gm-linker",
  "version": "1.0.0",
  "description": "Application to help GM developers discover information about technical skills",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "local": "webpack -w --display-error-details",
    "test": "karma start --reporters progress,kjhtml",
    "build": "rimraf dist && npm run build:en && npm run build:dev",
    "build:en": "webpack --config webpack/webpack.prod.en.js --progress --profile --bail",
    "build:dev": "webpack --config webpack/webpack.prod.dev.js --progress --profile --bail",
    "i18n": "ng-xi18n --i18nFormat=xlf -p ./src/tsconfig.json",
    "coverage": "karma start --single-run true"
  },
